class Solution:
    def twoSum(nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """

        indices_list = []


        
        for num1 in nums:
            num2 = target - num1
            if num2 in nums:
                indices_list.append(nums.index(num1))
                indices_list.append(nums.index(num2))

        for num1 in nums:
            

        return indices_list
    